{
    "name": "camelot/doctrine-sqlite-foreign-keys",
    "description": "Doctrine DBAL SQLite foreign key support",
    "keywords": ["doctrine", "dbal", "sqlite"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "doctrine/dbal": "^2.13 || ^3.0"
    },
    "require-dev": {
        "camelot/coding-style": "^3.0",
        "doctrine/orm": "^2.6 || ^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Camelot\\DoctrineSqliteForeignKeys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Camelot\\DoctrineSqliteForeignKeys\\Tests\\": "tests"
        }
    }
}
